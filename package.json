{
  "name": "botter",
  "version": "1.0.0",
  "description": "Bot for doing complicated stuff easy",
  "type": "module",
  "dependencies": {
    "csv-write-stream": "^2.0.0",
    "dotenv": "^16.0.1",
    "playwright": "^1.22.2",
    "uuid": "^8.3.2",
    "inquirer": "^9.0.0"
  },
  "scripts": {
    "generar": "node -r dotenv/config ./src/generar/main.js",
    "multi-generar": "node -r dotenv/config ./src/multiGenerador/index.js",
    "generar-vep": "node -r dotenv/config ./src/generarVep/generar-vep.js",
    "consultar": "node -r dotenv/config ./src/consultar/busquedaMensual.js.js",
    "consultar-anual": "node -r dotenv/config ./src/consultar/busquedaAnual.js",
    "test": "node -r dotenv/config test.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xaavito/botter.git"
  },
  "author": "Javier Martin Gonzalez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xaavito/botter/issues"
  },
  "homepage": "https://github.com/xaavito/botter#readme",
  "keywords": [
    "bot",
    "node",
    "playwright"
  ],
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  }
}
